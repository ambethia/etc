      "autocomplete"
      "linter-glsl"
      "autocomplete-glsl"
      "linter-haml"
      "language-glsl"
      "language-haml"
      "language-haxe"
      "linter-clang"
      "language-javascript"
      "linter-js-standard"
      "activate-power-mode"
      "automate-power-mode"
      "project-viewer"
      "linter-less"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "Thumbs.db"
      ".codekit-cache"
      "bower_components"
      "._*"
    ]
    projectHome: "/Users/ambethia/Documents"
  editor:
    invisibles:
      space: " "
    preferredLineLength: 120
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
